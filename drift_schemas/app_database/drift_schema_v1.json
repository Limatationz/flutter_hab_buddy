          "sql": "CREATE TABLE IF NOT EXISTS \"items_table\" (\"type\" TEXT NULL, \"oh_type\" TEXT NOT NULL, \"oh_name\" TEXT NOT NULL, \"oh_label\" TEXT NOT NULL, \"custom_label\" TEXT NULL, \"oh_category\" TEXT NULL, \"oh_tags\" TEXT NULL, \"oh_groups\" TEXT NULL, \"room_id\" INTEGER NULL REFERENCES rooms_table (id), \"is_favorite\" INTEGER NOT NULL DEFAULT 0 CHECK (\"is_favorite\" IN (0, 1)), \"icon\" TEXT NULL, \"score\" REAL NOT NULL DEFAULT 0.0, \"new_score\" REAL NOT NULL DEFAULT 0.0, \"manual_order_index\" INTEGER NOT NULL DEFAULT 0, \"complex_json\" TEXT NULL, PRIMARY KEY (\"oh_name\"));"
